package arrays;

import java.util.Scanner;

public class a1q3 {
	public static void main(String[] args)
	{
		System.out.println("enter the no of element of array");
		Scanner s = new Scanner(System.in);
		int n=s.nextInt();
		int a[]=new int[n];
		System.out.println("enter all elements");
		for(int i=0;i<n;i++)
		{
			a[i]=s.nextInt();
		}
		for (int i=n-1;i>=0;i--)
		{
			System.out.print(a[i]+" , ");
			
		}

	}}
